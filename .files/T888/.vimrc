syntax on
set expandtab
set tabstop=4
set nowrap
set encoding=utf-8
set fileencoding=utf-8
filetype plugin indent on
colorscheme ir_black
set pastetoggle=<F2>

" Used for vim-latexsuite
" filetype plugin indent on
set grepprg=grep\ -nH\ $*
let g:tex_flavor = "latex"
"set runtimepath=~/.vim,$VIM/vimfiles,$VIMRUNTIME,$VIM/vimfiles/after,~/.vim/after
